        <script>
            document.body.addEventListener("click", function (e) {
                $('.searchboxresult').empty();
                var a = $('.searchboxresult');
                a[0].parentElement.style.display = "none";
                $('.searchresidents').val("");

                if ($(e.target).closest('.AlertBox').length)
                    return;
                else {
                    var div_list = $('#ggg ul .list-group-item .subdiv');
                    var div_list2 = $('#ggg_next ul .list-group-item .subdiv');
                    for (var ind = 0; ind < div_list.length; ind++) {
                        div_list[ind].style.display = "none";
                    }
                    for (var ind = 0; ind < div_list2.length; ind++) {
                        div_list2[ind].style.display = "none";
                    }
                }


            });

            var ar = new Array(37, 38, 39, 40);
            var disableArrowKeys = function (e) {
                if ($.inArray(e.keyCode, ar) >= 0) {
                    e.preventDefault();
                }
            }
            $(document).keydown(disableArrowKeys);

            $(document).keydown(function (e) {

                var node;
                if (e.keyCode == 40 && $('.searchBox').css('display') == 'block') {
                    //alert("arrowdown pressed");
                    var check = 'false';
                    $('.searchboxresult a').each(function () {
                        if (this.style.backgroundColor == 'lightblue') {
                            check = 'true';
                            this.style.backgroundColor = '#fff';
                            if (this.nextElementSibling == null) {

                                node = $('.searchboxresult a:first');
                            }
                            else {
                                node = this.nextElementSibling;
                            }
                        }
                    })
                    if (check == 'false') {
                        $('.searchboxresult a:first').css('background-color', 'lightblue');
                        $(window).scrollTop(0);
                    }
                    else {
                        $(node).css('background-color', 'lightblue');
                        if ($(node).offset().top + $(node).height() > $(window).height() + $(window).scrollTop()) {
                            $("html, body").animate({ scrollTop: $(node).offset().top + $(node).height() - $(window).height() + 30 }, "fast");
                        }
                        else if ($(node).offset().top + $(node).height() < $(window).scrollTop() + 30) {
                            $("html, body").animate({ scrollTop: $(node).offset().top - $(node).height() - 20 }, "fast");
                        }
                        else {

                        }

                    }
                }
                else if (e.keyCode == 38 && $('.searchBox').css('display') == 'block') {

                    //alert("arrowdown pressed");
                    var check = 'false';
                    $('.searchboxresult a').each(function () {
                        if (this.style.backgroundColor == 'lightblue') {
                            check = 'true';
                            this.style.backgroundColor = '#fff';
                            if (this.previousElementSibling == null) {

                                node = $('.searchboxresult a:last');
                            }
                            else {
                                node = this.previousElementSibling;
                            }
                        }
                    })
                    if (check == 'false') {
                        $('.searchboxresult a:last').css('background-color', 'lightblue');
                        node = $('.searchboxresult a:last');
                    }
                    else {
                        $(node).css('background-color', 'lightblue');
                    }

                    if ($(node).offset().top < $(window).scrollTop() + 40) {
                        $(window).scrollTop($(node).offset().top - 60);
                    }
                    else if ($(node).offset().top > $(window).scrollTop() + $(window).height()) {
                        $("html, body").animate({ scrollTop: $(node).offset().top - $(window).height() + $(node).height() + 30 }, "fast");
                    }
                    else {

                    }

                }
                else if (e.keyCode == 13 && $('.searchBox').css('display') == 'block') {
                    $('.searchboxresult a').each(function () {
                        if (this.style.backgroundColor == 'lightblue') {
                            location.href = $(this).attr('href');
                        }
                    })
                }
            });




        </script>

