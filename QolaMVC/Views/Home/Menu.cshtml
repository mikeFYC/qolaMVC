@model QolaMVC.Models.HomeModel

@RenderPage("~/Views/Shared/_Header.cshtml")
@RenderPage("~/Views/Shared/_SideBar.cshtml")


<style>
        .badge {
            float: right;
            padding-left: 5px;
            padding-right: 5px;
        }
        .nobadge {
            float: none;
            /*margin-top: 30%;*/
            font-size: 0.8vw;
            display:inline-block;
            top:20%;
            width:80%;
            position:relative;
        }

        .one {
            width: 90% !important;
        }

        h5, h3 {
            font-weight: 500 !important;
            color: black;
        }

        .menu-box:hover .menu {
            transform: scale(1.05);
            transition: all .5s;
        }

        .pointer {
            cursor: pointer;
        }



        .list-group-item div:nth-child(3) {
            display: none;
            top: -10%;
            margin-left: -307%;
            position: absolute;
            z-index: 200;
            background-image: url('../../Content/pages/img/Rmenu_arrow.png');
            background-repeat: no-repeat;
            background-position: right;
            padding-right: 0.5vw;
            width: 50vw;
        }

        .list-group-item span {
            color: black;
        }

        .list-group-item div ul {
            list-style: none;
            float: right;
            position: static;
        }

        .list-group-item div ul li {
            padding: 0.5vw;
            width: 9vw;
            background-color: #FFF9A1;
            color: white;
            /*opacity: 0.7;*/
            float: right;
            height: 8vh;
            border-radius: 0.6vw;
            
        }
        .AlertBox .list-group-item div ul li {
            line-height: 1.5;
        }

        #ggg ul li[class=list-group-item] {
            padding: 0 !important;
        }

        #ggg ul li[class=list-group-item] a[class=one] {
            width: 18vw;
            padding-left: 1vw;
            padding-bottom: 0.8vw;
            padding-top: 0.8vw;
            padding-right: 1vw;
            z-index: 0;
            font-size: 1vw;
        }

        #ggg ul li[class=list-group-item] div ul li a {
            display: block;
            width: 8.5vw;
            min-height: 3vw;
            padding: 0 0.5vw 0 0 !important;
            font-size: 0.9vw;
        }

        #ggg_next ul li[class=list-group-item] {
            padding: 0 !important;
        }

        #ggg_next ul li[class=list-group-item] a[class=one] {
            width: 18vw;
            padding-left: 1vw;
            padding-bottom: 0.8vw;
            padding-top: 0.8vw;
            padding-right: 1vw;
            z-index: 0;
            font-size: 1vw;
        }

        #ggg_next ul li[class=list-group-item] div ul li a {
            display: block;
            width: 8.5vw;
            min-height: 3vw;
            padding: 0 0.5vw 0 0 !important;
            font-size: 0.9vw;
        }

    .hidefromBegin {
        display: none;
        font-size: 1vw !important;
        line-height: 1.5;
    }

    .hidefromBegin2 {
        display: none;
        font-size: 0.9vw !important;

    }

    .AlertBox {
        background-color: #FFF9A1;
        color: black;
        border-radius: 5%;
        width: 100%;
        margin-top: 0%;
        display: inline-block;
    }

    #ggg .hidefromBegin, #ggg_next .hidefromBegin {
        background-color: transparent;
        color: #015093;
        border-top: 1px solid skyblue;
        padding: 0;
    }

    #ggg .hidefromBegin li a, #ggg_next .hidefromBegin li a {
        color: black;
    }

    .leftnumber {
        width: 17%;
        height: 6vh;
        display: inline-block;
        padding: 0px;
        border-right: 3px solid #FFB380;
        vertical-align:top;
        text-align:center;
    }
    .FirstLayerToDoListText {
        width: 77%;
        height: 100%;
        display: inline-block;
        padding: 6px;
    }
