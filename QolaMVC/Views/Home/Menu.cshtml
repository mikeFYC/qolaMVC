<style>
    div[class="float_box"] ul[class="list-group"] li:hover {
        background-color: grey !important;

    }

    div[id="Table60"] div div div[class="modal-body"] table tbody tr {
        cursor: pointer;
    }

        div[id="Table60"] div div div[class="modal-body"] table tbody tr:hover {
            background-color: grey;
        }

    div div div div[class="modal-body"] {
        overflow: auto;
        overflow-x: hidden;
        max-height: 30vw;
    }

    .miketable {
        width: 100%;
        color: black;
    }

        .miketable thead th {
            padding: 1vw;
        }

        .miketable tbody tr {
            height: 3vw;
        }

            .miketable tbody tr td {
                padding: 1vw;
            }
</style>

<style>
    #contactForm {
        display: none;
        border: 6px solid salmon;
        padding: 2em;
        width: 30vw;
        height: 40vw;
        text-align: center;
        background: #fff;
        position: fixed;
        top: 50%;
        left: 50%;
        transform: translate(-50%,-50%);
        -webkit-transform: translate(-50%,-50%)
    }

    .formBtn, .formtext {
        margin: .8em auto;
        font-family: inherit;
        text-transform: inherit;
        font-size: inherit;
        display: block;
        padding: .4em;
    }

    .formtext {
        resize: none;
    }

    .formBtn {
        display: block;
        background: teal;
        color: #fff;
        font-weight: 100;
        font-size: 1.2em;
        border: none;
        width: 15vw
    }

    .formtext2 {
        min-height: 10vw;
        min-width: 10vw;
    }
