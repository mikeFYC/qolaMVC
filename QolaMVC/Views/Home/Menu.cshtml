        <script>

            function nextlayer(ee) {
                var li_list = ee.parentElement.parentElement.children;
                for (var ind = 0; ind < li_list.length; ind++) {
                    if (li_list[ind].firstElementChild != ee)
                        li_list[ind].lastElementChild.style.display = "none";
                }
                var next = ee.nextElementSibling.nextElementSibling;
                if (next.style.display == "block") {
                    next.style.display = "none";
                }
                else if (next.style.display == "none" || next.style.display == "") {
                    next.style.display = "block";
                }
            }

