                                //l_Categories = data;
                              //console.log(data);

                                for (var i = 0; i < data.length; i++ ) {
                                    l_Categories[data[i].Id] = data[i].Name;
                                }
                                console.log(l_Categories);
                                initCalendar();
                            },
                            error: function (ex) {
                                var r = jQuery.parseJSON(response.responseText);
                                alert("Message: " + r.Message);
                                alert("StackTrace: " + r.StackTrace);
                                alert("ExceptionType: " + r.ExceptionType);
                            }
                        });



                        function initCalendar() {
                             $('#calendar').CalendarPlugin({
                                 url: '/Home/getEvents',
                                 view: 'daily',
                                 deleteUrl: 'http://localhost:8040/php/events.php?type=delete&id=:id',
                                 editUrl: 'http://localhost:8040/php/events.php?type=edit&id=:id',

                            form: [
                                [
                                    {
                                        type: 'text',
                                        name: 'title',
                                        label: 'Activity Display Title',
                                        placeholder: 'E.g. Marathon'
                                    }

                                ],
                                [
                                    {
                                        type: 'select',
                                        name: 'activity',
                                        label: 'Activity *',
                                        placeholder: 'Select',
                                        options: l_Categories
                                    }
                                ],
                                [
                                    {
                                        type: 'radio',
                                        name: 'frequency',
                                        label: 'Frequency',
                                        defaultValue: 1,
                                        options: { 1: 'Date & Time', 2: 'Every Weekday', 3: 'Date between' }
                                    }
                                ],
                                [
                                    {
                                        type: 'datetime',
                                        name: 'startDate',
                                        label: 'Date & Time',
                                        placeholder: 'DD/MM/YYYY',
                                        bindTo: {
                                            name: 'input[name="frequency"]',
                                            when: '1'
