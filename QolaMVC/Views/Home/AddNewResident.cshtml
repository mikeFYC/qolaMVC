<script>
    function restore_picture() {
        var a = $('#blah');
        a[0].src = "/Content/pages/img/photo_dummysize.PNG";
        var b = $('.upload__link');
        var c = b[0].getElementsByTagName("input");
        c[0].click(function () { })
    }

    function check_1st() {
        var Gendar = $('input[name="Gendar"]');
        var no_phone = $('#gridCheck1');

        if ($('#FirstName').val() == "") {
            tempAlert("FirstName Needed", 2000);
            return false;
        }
        else if ($('#LastName').val() == "") {
            tempAlert("LastName Needed", 2000);
            return false;
        }
        else if ($('#DOB').val() == "") {
            tempAlert("DOB Needed", 2000);
            return false;
        }
        else if (Gendar[0].checked == false && Gendar[1].checked == false) {
            tempAlert("Gendar Needed", 2000);
            return false;
        }
        else if ($('#phonenumber').val() == "" && no_phone[0].checked==false) {
            tempAlert("Phone Needed", 2000);
            return false;
        }
        else if ($('#MaritalStatus').val() == "") {
            tempAlert("MaritalStatus Needed", 2000);
            return false;
        }
        else if ($('#MoveInDate').val() == "") {
            tempAlert("MoveInDate Needed", 2000);
            return false;
        }
        else if ($('#Occupancy').val() == "") {
            tempAlert("Occupancy Needed", 2000);
            return false;
        }
        else if ($('#AvailableSuites').val() == "") {
            tempAlert("AvailableSuites Needed", 2000);
            return false;
        }
        else if ($('#MBhealthNumber').val() == "") {
            tempAlert("HealthNumber Needed", 2000);
            return false;
        }
        else {
            return true;
        }
    }

    function check_2nd() {
        var POACareStatus = $('input[name="POACareStatus"]');
        var POAFinanceStatus = $('input[name="POAFinanceStatus"]');

        var hos = $('#CallHospital');
        if (hos[0].checked == true) {
            return true;
        }
        else {
            if ($('#Contract1').val() == "") {
                tempAlert("Contact1 Name Needed", 2000);
                return false;
            }
            else if ($('#Address1').val() == "") {
                tempAlert("Contact1 Address Needed", 2000);
                return false;
            }
            else if ($('#Relationship1').val() == "") {
                tempAlert("Contact1 Relationship Needed", 2000);
                return false;
            }
            else if ($('#First_phone1').val() == "") {
                tempAlert("Contact1 Primary Phone Needed", 2000);
                return false;
            }
            else if ($('#First_phone_type1').val() == "") {
                tempAlert("Contact1 Primary Phone Type Needed", 2000);
                return false;
            }
            else if ($('#Email1').val() == "") {
                tempAlert("Contact1 Email Needed", 2000);
                return false;
            }
            else if (POACareStatus[0].checked == false && POACareStatus[1].checked == false) {
                tempAlert("Contact1 POA Care Needed", 2000);
                return false;
            }
            else if (POAFinanceStatus[0].checked == false && POAFinanceStatus[1].checked == false) {
                tempAlert("Contact1 POA Finance Needed", 2000);
                return false;
            }
            else {
                return true;
            }

        }
    }

    function check_3rd() {
        if ($('#HealthHistory').val() == "") {
            tempAlert("Medical History Needed", 2000);
            return false;
        }
        else if ($('#CurrentDiagnoses').val() == "") {
            tempAlert("Current Diagnoses Needed", 2000);
            return false;
        }
        else if ($('#Physician').val() == "") {
            tempAlert("Physician Name Needed", 2000);
            return false;
        }
        else if ($('#PhysicianPhone').val() == "") {
            tempAlert("Physician Phone Needed", 2000);
            return false;
        }
        else if ($('#PharmacyName').val() == "") {
            tempAlert("Pharmacy Name Needed", 2000);
            return false;
        }
        else if ($('#PharmacePhoneNo').val() == "") {
            tempAlert("Pharmace Phone Needed", 2000);
            return false;
        }
        else {
            return true;
        }
    }

    function goto_second() {
        if (check_1st()==true){
            document.getElementById("pills-home-tab").className = "nav-link";
            document.getElementById("pills-profile-tab").className = "nav-link active";
            document.getElementById("pills-home").className = "tab-pane fade";
            document.getElementById("pills-profile").className = "tab-pane fade show active";
            document.body.scrollTop = 0;
            document.documentElement.scrollTop = 0;
        }
    }

    function goto_third() {
        if (check_1st() == true && check_2nd() == true) {
            document.getElementById("pills-profile-tab").className = "nav-link";
            document.getElementById("pills-contact-tab").className = "nav-link active";
            document.getElementById("pills-profile").className = "tab-pane fade";
            document.getElementById("pills-contact").className = "tab-pane fade show active";
            document.body.scrollTop = 0;
            document.documentElement.scrollTop = 0;
        }


    }

    function goto_last() {
        document.getElementById("pills-home-tab").className = "nav-link";
        //document.getElementById("pills-profile-tab").className = "nav-link";
        //document.getElementById("pills-contact-tab").className = "nav-link";
        document.getElementById("pills-other-tab").className = "nav-link active";
        document.getElementById("pills-home").className = "tab-pane fade";
        //document.getElementById("pills-profile").className = "tab-pane fade";
        //document.getElementById("pills-contact").className = "tab-pane fade";
        document.getElementById("pills-other").className = "tab-pane fade show active";
    }

    function back_first() {
        document.getElementById("pills-profile-tab").className = "nav-link";
        document.getElementById("pills-home-tab").className = "nav-link active";
        document.getElementById("pills-profile").className = "tab-pane fade";
        document.getElementById("pills-home").className = "tab-pane fade show active";
        document.body.scrollTop = 0;
        document.documentElement.scrollTop = 0;
    }

    function back_second() {
        document.getElementById("pills-contact-tab").className = "nav-link";
        document.getElementById("pills-profile-tab").className = "nav-link active";
        document.getElementById("pills-contact").className = "tab-pane fade";
        document.getElementById("pills-profile").className = "tab-pane fade show active";
        document.body.scrollTop = 0;
        document.documentElement.scrollTop = 0;
    }

</script>


