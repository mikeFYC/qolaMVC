            function readURL(input) {
                if (input.files && input.files[0]) {
                    var reader = new FileReader();

                    reader.onload = function (e) {
                        $('#blah')
                            .attr('src', e.target.result)
                        //.width(150)
                        //.height(200);
                    };

                    reader.readAsDataURL(input.files[0]);
                }
            }

            function getAvailableSuites() {
                var occupancy = $("#Occupancy").val();
                var moveInDate = $("#MoveInDate").val();
                var availableSuites = $("#AvailableSuites").val();

                if (occupancy == 0 && moveInDate != "") {
                    //alert("Please Select a valid occupancy option");
                    tempAlert("Choose an occupancy option", 2000);
                }

                if (occupancy > 0 && moveInDate == "") {
                    //alert("Please Select a valid move in date");
                    tempAlert("Choose a valid move in date", 2000);
                }

                if (occupancy > 0 && moveInDate != "") {
                    $("#AvailableSuites").attr("disabled", false);

                    var a = $('#MoveInDate').val();
                    var b = $('#Occupancy').val();

                    $('#AvailableSuites').children('option:not(:first)').remove();
                    $.get("/Residents/AvailableSuite", { term: a, occu: b }, function (data) {
                        for (var i = 0; i < data.length; i++) {
                            $('#AvailableSuites').append('<option value="' + data[i][0].Value + '">' + data[i][0].Value + '</option>');
                        }
                    });
                }
            }
        </script>

