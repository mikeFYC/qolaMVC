                var hos = $('#CallHospital')
                if (hos[0].checked == true) {
                    $('div[name="contactInformation"] input').each(function () {
                        this.disabled = true;
                    })
                    $('div[name="contactInformation"] select').each(function () {
                        this.disabled = true;
                    })
                }
                else {
                    $('div[name="contactInformation"] input').each(function () {
                        this.disabled = false;
                    })
                    $('div[name="contactInformation"] select').each(function () {
                        this.disabled = false;
                    })
                }
            });

            $('form').submit(function (event) {
                if (check_1st() == true && check_2nd() == true && check_3rd()==true) {
                    form = this;
                    form.submit();
                }
                else {
                    check_1st();
                    check_2nd();
                    check_3rd();
                    return false;
                }
            });

            $('#gridCheck1').click(function () {
                var phoneindex = $('#gridCheck1');
                if (phoneindex[0].checked == true) {
                    $('#phonenumber').val("");
                }
            });

        </script>
