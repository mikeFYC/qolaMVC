                                </div>
                                <div class="Row" style="padding: 5px;">
                                    <a href="#" style="padding: 5px;" class="btn btn-primary" onclick="form_click(this)">Forms</a>
                                    <a href="~/Home/PersonalCalendar" style="padding: 5px;" class="btn btn-primary">Personal Calendar</a>
                                    @{
                                        if (@TempData["archive"].ToString() == "NO")
                                        {
                                            <a href="~/Home/SuggestedActivityCalendar" id="SAC_a_tag" style="padding: 5px;" class="btn btn-primary">Suggested Activity Calendar</a>
                                        }
                                    }

                                </div>
                                @{
                                    if (@TempData["pass"].ToString() == "" && @TempData["archive"].ToString() == "YES")
                                    {
                                        <div id="Restore_RP" class="Row" style="padding:5px;">
                                            <a class="btn btn-primary" href="#" data-toggle="modal" data-target="#Restore_Resident" style="padding:5px;background-color:#f55753;border-color:#f55753;">Restore Resident Profile</a>
                                        </div>
                                    }
                                }

