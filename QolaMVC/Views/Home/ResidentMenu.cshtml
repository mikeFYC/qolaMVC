                <div class="container-fluid container-fixed-lg">
                    <!-- <div class="inner"> -->
                    <div class="align-items-center" style="width:100% !important;">
                        <div class="col-md-9">
                            <h3>Resident Profile</h3>
                        </div>
                        @{
                            if (@TempData["archive"].ToString() == "YES")
                            {
                                <div id="Resident_Archive_label" class="align-items-center" style="float:right;border:black solid;padding-left:1vw;padding-right:1vw;">
                                    <h3>Resident Archive</h3>
                                </div>
                            }
                        }



                    </div>
                    <!-- </div> -->
                </div>
            </div>
            <!-- END JUMBOTRON -->
            <div style="height: 60px;"></div>
