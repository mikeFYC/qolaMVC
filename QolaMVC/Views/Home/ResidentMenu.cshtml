                function DELETEDOCUMENT(ee, documentId, documentTypeName) {
                    ee.parentNode.parentNode.parentNode.removeChild(ee.parentNode.parentNode);
                    $.ajax({
                        url: '/Documents/DeleteDocument?DocumentId=' + documentId + "&DocumentTypeName=" + documentTypeName,
                        type: 'post',
                        dataType: 'json',
                        contentType: 'application/json; charset=utf-8',
                        success: function (data) {
                            if (data == "Success") {
                                $('#trDocument_' + documentId).remove();
                                tempAlert("Delete Successfully", 2000);
                            }
                            else {
                                tempAlert(data, 2000);
                            }
                        },
                        error: function (errorThrown) {

                        }
                    });
                }

                function deleteAlert(msg, id, docTypeName) {
                    var el = document.createElement("div");
                    el.setAttribute("style", "position:fixed;top:2%;left:31%;width:35vw;background-color:white;z-index:9999;color:black;padding:2vw 3vw 2vw 3vw;border-radius:1vw;font-size:1.2em;font-weight:bold;border:black solid;");
                    el.innerHTML = "<img src=\"\/Content\/assets\/img\/qola_logo3.png\" style=\"width:120px;height:80px;display:inline-block\" \/><div style=\"margin-left:3vw;display:inline-block;width:50%;position:absolute;margin-top:3%;\">" + msg + "<\/div><div style='width:90%;'><button class='btn btn-primary pull-right' style='display:inline-block;' onclick='this.parentNode.parentNode.parentNode.removeChild(this.parentNode.parentNode);'>No<\/button><button class='btn btn-danger pull-right' style='display:inline-block;margin-right:10px' onclick=\"DELETEDOCUMENT(this, " + id + ", " + docTypeName + ")\">Yes<\/button><\/div>";
                    document.body.appendChild(el);
                }

