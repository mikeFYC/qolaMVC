@model QolaMVC.Models.ResidentModel

@RenderPage("~/Views/Shared/_Header.cshtml")
@RenderPage("~/Views/Shared/_SideBar.cshtml")
<style>

    .menu-box:hover .menu {
        transform: scale(1.05);
        transition: all .5s;
    }

    .pointer {
        cursor: pointer;
    }

    .nav-link.active {
        background-color: #015093 !important;
    }

    #options_in_profile button {
        border-radius: 0.3vw;
    }

    #options_in_profile a {
        border-radius: 0.3vw;
    }


    #pills-care .ptitle {
        font-family: Times,serif;
        font-size: 15px;
        color: rgb(0,0,0);
        font-weight: bold;
        font-style: normal;
        text-decoration: none;
    }
    #pills-care p {
        margin-bottom:5px !important;
    }
    #pills-care .ptitle_grey {
        font-family: Times,serif;
        font-size: 15px;
        color: rgb(0,0,0);
        font-weight: bold;
        font-style: normal;
        text-decoration: none;
        background-color: darkgrey;
        border-bottom: 1px solid black;
    }
    #pills-care .ptitle_nocolor_noborderB {
        font-family: Times,serif;
        font-size: 15px;
        color: rgb(0,0,0);
        font-weight: bold;
        font-style: normal;
        text-decoration: none;
    }
    #pills-care .ptitle_color_noborderB {
        font-family: Times,serif;
        font-size: 15px;
        color: rgb(0,0,0);
        font-weight: bold;
        font-style: normal;
        text-decoration: none;
        background-color: darkgrey;
    }
    .borderB {
        border-bottom: 1px solid black;
    }

    #pills-care .pcontent {
        font-family: Times,serif;
        font-size: 15px;
        color: rgb(0,0,0);
        font-weight: normal;
        font-style: normal;
        text-decoration: none;
    }
    .title2 {
        color: black !important;
        font-size: 1.5em !important;
        font-family: Times,serif !important;
    }

    .title1 {
        font-weight: bold !important;
        color: black !important;
        font-size: 1.7em !important;
        font-family: Times,serif !important;
    }
    .pagebreak {
        page-break-before: always;
    }

</style>

<!-- START PAGE-CONTAINER -->
<div class="page-container">

    @RenderPage("~/Views/Shared/_NavBar.cshtml")


    <!-- START PAGE CONTENT WRAPPER -->
    <div class="page-content-wrapper">
        <!-- START PAGE CONTENT -->
        <div class="content">
            <!-- START JUMBOTRON -->
            <div class="jumbotron w-100" style="position: fixed; z-index: 400">
                <div class="container-fluid container-fixed-lg">
                    <!-- <div class="inner"> -->
                    <div class="align-items-center" style="width:100% !important;">
                        <div class="col-md-9">
                            <h3>Resident Profile</h3>
                        </div>
                        @{
                            if (@TempData["archive"].ToString() == "YES")
                            {
                                <div id="Resident_Archive_label" class="align-items-center" style="float:right;border:black solid;padding-left:1vw;padding-right:1vw;">
                                    <h3>Resident Archive</h3>
                                </div>
                            }
                        }



                    </div>
                    <!-- </div> -->
                </div>
            </div>
            <!-- END JUMBOTRON -->
            <div style="height: 60px;"></div>
            <!-- START CONTAINER FLUID -->
            <div class="container-fluid container-fixed-lg">
                <!-- BEGIN PlACE PAGE CONTENT HERE -->
                <div class="card">
                    <div class="card-block">
                        <div class="row">
                            <div class="" style="width:48% !important">
                                <div class="row">
                                    <div class="" style="width:33% !important;">
                                        <img id="image" src="~/Content/assets/@Model.ResidentImage" class="img-responsive" style="margin-left:10%;height:70%;width:50%;">
                                        <button id="rotatebutton" class="btn btn-default btn-group-sm" style="margin-left:10%;"><i class="fa fa-rotate-right"></i></button>
                                        <button id="rotatebutton2" class="btn btn-default btn-group-sm"><i class="fa fa-rotate-left"></i></button>
                                        <input  id="edit_photo" type="file" style="display:none;" onchange="EDIT_PHOTO()" />
                                        <button class="btn btn-sm" id="upfile1" style="cursor:pointer;margin-left:10%;z-index:100;">EDIT PIC</button>
                                    </div>
                                    <div class="" style="width:66% !important;">
                                        <br>
                                        <div class="row">
                                            <div class="" style="width:48% !important;">
                                                @{
                                                    if (Model.ShortName != null && Model.ShortName != "")
                                                    {
                                                        <p style="font-size:1.3vw !important;">@Model.FirstName "@Model.ShortName" @Model.LastName</p>
                                                    }
                                                    else
                                                    {
                                                        <p style="font-size:1.3vw !important;">@Model.FirstName  @Model.LastName</p>
                                                    }
                                                }

                                                <p style="font-size:1.3vw !important;">@Model.Gendar</p>
                                                <p style="font-size:1.3vw !important;">@Model.BirthDate.ToShortDateString()</p>
                                                @{
                                                    if (TempData["pass"] != null && TempData["pass"].ToString() != "")
                                                    {
                                                        <p style="font-size:1.3vw !important;width:15vw;">Pass Away : <span>@TempData["pass"]</span></p>
                                                    }
                                                }
                                            </div>
                                            <div class="" style="width:48% !important;">
                                                <p style="font-size:1.3vw !important;width:25vw;">@Model.Home.Name</p>
                                                <p style="font-size:1.3vw !important;">@Model.SuiteNo</p>
                                                <p style="font-size:1.3vw !important;">@Model.Physician</p>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="text-right" style="width:48% !important" id="options_in_profile">
                                <div class="Row" style="padding: 5px;">
                                    <a href="/Home/ERDetails" style="padding: 5px;" class="btn btn-danger">Print Emergency Resident Details</a>
                                </div>
                                <div class="Row" style="padding: 5px;">
                                    <button style="padding: 5px;" class="btn btn-primary" onclick="QOLAFIED()">Print QOLAFIED Assessment</button>
                                    @*<a href="/Reports/" style="padding: 5px;" class="btn btn-primary">Assessment Forms (Left here for demo only)</a>*@

                                </div>
                                <div class="Row" style="padding: 5px;">
                                    <a href="#" style="padding: 5px;" class="btn btn-primary" onclick="form_click(this)">Forms</a>
                                    <a href="~/Home/PersonalCalendar" style="padding: 5px;" class="btn btn-primary">Personal Calendar</a>
                                    @{
                                        if (@TempData["archive"].ToString() == "NO")
                                        {
                                            <a href="~/Home/SuggestedActivityCalendar_mike" id="SAC_a_tag" style="padding: 5px;" class="btn btn-primary">Suggested Activity Calendar</a>
                                        }
                                    }

                                </div>
                                @{
                                    if (@TempData["pass"].ToString() == "" && @TempData["archive"].ToString() == "YES")
                                    {
                                        <div id="Restore_RP" class="Row" style="padding:5px;">
                                            <a class="btn btn-primary" href="#" data-toggle="modal" data-target="#Restore_Resident" style="padding:5px;background-color:#f55753;border-color:#f55753;">Restore Resident Profile</a>
                                        </div>
                                    }
                                }

                                <div class="modal fade" id="Restore_Resident" role="dialog" style="right:0">
                                    <div class="modal-dialog" style="left:0;right:0;">
                                        <div class="modal-content" style="width:60vw;right:45%;margin-top:-3.6vw;">

                                            <div class="modal-header">
                                                <button type="button" class="close" data-dismiss="modal">&times;</button>
                                                <h3 class="modal-title" style="font-weight:500 !important;color:black;">Restore Resident Profile</h3>
                                                <br>
                                            </div>
                                            <div class="modal-body">
                                                <div id="ck" class="">
                                                    <div class="" style="padding:15px;display:block;">
                                                        <a href="#" title="" class="float-left">
                                                            <img id="QOLAContent_ImgResidentReturn" src="~/Content/assets/@Model.ResidentImage" style="height:120px;width:100px;">
                                                        </a>

                                                        <div class="residentInfo" style="margin-left:9vw;">
                                                            <table class="TabletTable" style="font-size:1.3vw;">
                                                                <tbody>
                                                                    <tr>
                                                                        <td align="left">
                                                                            Name :
                                                                            <span id="QOLAContent_lResiName">@Model.FirstName @Model.LastName</span>
                                                                        </td>
                                                                    </tr>
                                                                    <tr>
                                                                        <td align="left">
                                                                            Gender :
                                                                            <span id="QOLAContent_lGender">@Model.Gendar</span>
                                                                        </td>
                                                                    </tr>
                                                                    <tr>
                                                                        @{
                                                                            if (TempData["pass"] == null || TempData["pass"].ToString() == "")
                                                                            {
                                                                                <td align="left">
                                                                                    Age :
                                                                                    <span id="QOLAContent_lAge">@Model.Age (@Model.BirthDate.ToShortDateString())</span>
                                                                                </td>
                                                                            }
                                                                            else
                                                                            {
                                                                                <td align="left">
                                                                                    Pass Away :
                                                                                    <span id="QOLAContent_lAge">@TempData["pass"]</span>
                                                                                </td>
                                                                            }
                                                                        }

                                                                    </tr>
                                                                </tbody>
                                                            </table>
                                                        </div>
                                                    </div>

                                                    <div style="border: 2px solid gray; padding: 15px; width: 100%; display:inline-block;margin-top:2vw;">

                                                        <div class="tab-pane fade show active" id="pills-InternalTransfer" role="tabpanel" aria-labelledby="pills-contact-tab">
                                                            <br>
                                                            <br>
                                                            <div class="row">
                                                                <div class="col-md-6">
                                                                    <div class="form-group row">
                                                                        <label for="staticEmail" class="col-md-4 col-form-label">Return Date</label>
                                                                        <div class="col-md-6">
                                                                            <input type="date" class="form-control" id="dateinput_archive" onchange="selection_onchange_empty_suite()">
                                                                        </div>
                                                                        <div class="col-md-2">
                                                                            <p style="font-size:9px;float:left;margin-left:-1vw;">(MM/DD/YYYY)</p>
                                                                        </div>
                                                                    </div>

                                                                    <div class="form-group row" style="margin-top:1.5vw;">
                                                                        <label for="staticEmail" class="col-md-4 col-form-label">Suite #</label>
                                                                        <div class="col-md-6">
                                                                            <select id="available_Suite_archive" class="form-control" onfocus="suitenumber3()">
                                                                                <option value="">--Suite--</option>
                                                                            </select>
                                                                        </div>
                                                                    </div>

                                                                    <div class="form-group row" style="margin-top:1.5vw;">
                                                                        <label for="staticEmail" class="col-md-4 text-right col-form-label">Reason</label>
                                                                        <div class="col-md-6">
                                                                            <select id="reason_archive" tabindex="2" class="" style="height:35px;width:100%;float:left;">
                                                                                <option selected="selected" value="">-- Select --</option>
                                                                                <option value="Personal Leave">Personal Leave</option>
                                                                                <option value="Medical Leave">Medical Leave</option>
                                                                                <option value="Reason Unknown">Reason Unknown</option>
                                                                            </select>
                                                                        </div>
                                                                    </div>
                                                                </div>

                                                                <div class="col-md-6">
                                                                    <div class="form-group row">
                                                                        <label for="staticEmail" class="col-md-4 text-right col-form-label">Occupancy</label>
                                                                        <div class="col-md-6">
                                                                            <select id="occupancy_archive" class="form-control" onchange="selection_onchange_empty_suite()">
                                                                                <option value="">--Select--</option>
                                                                                <option value="1">Single</option>
                                                                                <option value="2">Double</option>
                                                                                <option value="3">Triple</option>
                                                                            </select>
                                                                        </div>
                                                                    </div>

                                                                    <div class="form-group row" style="margin-top:1.5vw;">
                                                                        <label for="staticEmail" class="col-md-4 text-right col-form-label">Resident Image</label>
                                                                        <div class="col-md-7">
                                                                            <input type="file" id="img_archive" />
                                                                        </div>
                                                                    </div>
                                                                </div>



                                                                <div class="col-md-12" style="margin-top:1.5vw;">
                                                                    <div class="form-group row">
                                                                        <label for="staticEmail" class="col-md-2 col-form-label">Notes</label>
                                                                        <div class="col-md-9">
                                                                            <textarea class="textarea" id="notes_archive" rows="5" style="width:100% !important;max-width:100%;float:left;"></textarea>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </div>

                                                        </div>


                                                    </div>
                                                </div>
                                            </div>
                                            <div class="modal-footer">
                                                <br>
                                                <div class="row">
                                                    <div class="col-md-12 text-center">
                                                        @*<button class="btn btn-danger" data-dismiss="modal">
                                                                BACK
                                                            </button>*@
                                                        <button class="btn btn-danger" onclick="saveButton_archive()" style="width:6vw;">
                                                            SAVE
                                                        </button>
                                                        <button class="btn btn-danger" onclick="clearButton()" style="width:6vw;">
                                                            CLEAR
                                                        </button>
                                                    </div>
                                                </div>

                                            </div>

                                        </div>
                                    </div>
                                </div>





                            </div>
                            <br>
                            @{
                                if (ViewBag.Message != null && ViewBag.Message != string.Empty)
                                {
                                    <div class="alert alert-success" role="alert">
                                        <button class="close" data-dismiss="alert"></button>
                                        <strong>Success: </strong> @ViewBag.Message
                                    </div>
                                }
                            }

                            <div class="row" style="min-width:95%;padding-bottom:80px">
                                <div class="col-md-12">

                                    <div class="form-tabs mt-5">
                                        <ul class="nav nav-pills mb-3" id="pills-tab" role="tablist">
                                            <li class="nav-item">
                                                <a class="nav-link active" id="pills-home-tab" data-toggle="pill" href="#pills-home" role="tab" aria-controls="pills-home" aria-selected="true" onclick="tab_click(this)"><b>General Info</b> </a>
                                            </li>
                                            <li class="nav-item" style="display:none;">
                                                <a class="nav-link" id="pills-medication-tab" data-toggle="pill" href="#pills-medication" role="tab" aria-controls="pills-medication" aria-selected="false" onclick="tab_click(this)"><b>Medication History</b></a>
                                            </li>
                                            <li class="nav-item">
                                                <a class="nav-link" id="pills-care-tab" data-toggle="pill" href="#pills-care" role="tab" aria-controls="pills-care" aria-selected="false" onclick="tab_click(this)"><b>Plan of Care</b></a>
                                            </li>
                                            @*<li class="nav-item">
                                                <a class="nav-link" id="pills-goals-tab" data-toggle="pill" href="#pills-goals" role="tab" aria-controls="pills-goals" aria-selected="false"><b>Resident Specific Goals</b></a>
                                            </li>*@
                                            <li class="nav-item">
                                                <a class="nav-link" id="pills-notes-tab" data-toggle="pill" href="#pills-notes" role="tab" aria-controls="pills-notes" aria-selected="false" onclick="tab_click(this)"><b>Progress Notes</b></a>
                                            </li>
                                            <li class="nav-item" style="display:none;">
                                                <a class="nav-link" id="pills-story-tab" data-toggle="pill" href="#pills-story" role="tab" aria-controls="pills-story" aria-selected="false" onclick="tab_click(this)"><b>Life Story Summary</b></a>
                                            </li>

                                            <li class="nav-item">
                                                <button class="btn btn-danger" id="print_PoC" onclick="PoC_PRINT()" style="border-radius:0.3vw;height:40px;margin-left:30%;display:none;">Print PoC</button>
                                            </li>

                                        </ul>
                                        <div class="tab-content" id="pills-tabContent">
                                            <div class="tab-pane fade show active" id="pills-home" role="tabpanel" aria-labelledby="pills-home-tab">

                                                <table class="table table-bordered">
                                                    <thead class="bg-primary">
                                                        <tr>
                                                            <th class="text-white text-center" colspan="3">Resident Details</th>
                                                        </tr>
                                                    </thead>
                                                    <tbody>

                                                        <tr>
                                                            @{
                                                                if (Model.ShortName != null && Model.ShortName != "")
                                                                {
                                                                    <td>Name: @Model.FirstName "@Model.ShortName" @Model.LastName</td>
                                                                }
                                                                else
                                                                {
                                                                    <td>Name: @Model.FirstName @Model.LastName</td>
                                                                }
                                                            }
                                                            @{
                                                                if (Model.Gendar == 'F')
                                                                {
                                                                    <td>Gender: Female</td>
                                                                }
                                                                else
                                                                {
                                                                    <td>Gender: Male</td>
                                                                }
                                                            }


                                                            <td>Date Of Birth: @Model.BirthDate.ToString("yyyy-MM-dd")</td>
                                                        </tr>
                                                        <tr>
                                                            <td>Phone Number: @Model.Phone </td>
                                                            <td>Health Card #: @Model.MBhealthNumber</td>
                                                            <td>Primary Language: @Model.OtherLanguage</td>
                                                        </tr>
                                                        <tr>
                                                            <td>Marital Status: @Model.MaritalStatustext </td>
                                                            <td>Significant Other: @Model.SignificatOther </td>
                                                            @{ 
                                                                var AnnDate = "";
                                                                if(@Model.AnniversaryDate.ToString("yyyy-MM-dd") != "0001-01-01")
                                                                {
                                                                    AnnDate = @Model.AnniversaryDate.ToString("yyyy-MM-dd");
                                                                }
                                                            }
                                                            <td>Anniversity Date: @AnnDate</td>
                                                        </tr>
                                                        <tr>
                                                            <td>Number of Children: @Model.Number_of_children</td>
                                                            <td>Number of Grandchildren: @Model.Number_of_grandchildren </td>
                                                            <td>Veteran: @Model.Vetaran</td>
                                                        </tr>
                                                        <tr style="display:none;">
                                                            <td>Contract Number: @Model.ContractNumber</td>
                                                            <td colspan="2">POA Care: @Model.POACare</td>
                                                        </tr>
                                                        <tr style="display:none;">
                                                            <td colspan="3">POA Finance: @Model.POAFinance</td>
                                                        </tr>
                                                        <tr style="display:none;">
                                                            <td>Finance Home Phone: @Model.FinanceHomePhone</td>
                                                            <td>Finance Work Phone: @Model.FinanceWorkPhone</td>
                                                            <td>Finance Email: @Model.FinanceEmail</td>
                                                        </tr>
                                                    </tbody>
                                                </table>
                                                <br><br>
                                                <table class="table table-bordered">
                                                    <thead class="bg-primary">
                                                        <tr>
                                                            <th class="text-white text-center" colspan="4">Emergency Contact Details</th>
                                                        </tr>
                                                    </thead>
                                                    <tbody>
                                                        <tr>
                                                            @{
                                                                var POAC1 = ""; var POAF1 = "";
                                                                var POAC2 = ""; var POAF2 = "";
                                                                var POAC3 = ""; var POAF3 = "";
                                                                if (Model.POACareStatus == 'Y') { POAC1 = "POA Care"; } else { POAC1 = "\xa0\xa0\xa0\xa0\xa0\xa0\xa0\xa0"; }
                                                                if (Model.POACareType2Status == 'Y') { POAC2 = "POA Care"; } else { POAC2 = "\xa0\xa0\xa0\xa0\xa0\xa0\xa0\xa0"; }
                                                                if (Model.POACareType3Status == 'Y') { POAC3 = "POA Care"; } else { POAC3 = "\xa0\xa0\xa0\xa0\xa0\xa0\xa0\xa0"; }
                                                                if (Model.POAFinanceStatus == 'Y') { POAF1 = "POA Finance"; } else { POAF1 = "\xa0\xa0\xa0\xa0\xa0\xa0\xa0\xa0"; }
                                                                if (Model.POAFinanceType2Status == 'Y') { POAF2 = "POA Finance"; } else { POAF2 = "\xa0\xa0\xa0\xa0\xa0\xa0\xa0\xa0"; }
                                                                if (Model.POAFinanceType3Status == 'Y') { POAF3 = "POA Finance"; } else { POAF3 = "\xa0\xa0\xa0\xa0\xa0\xa0\xa0\xa0"; }
                                                            }
                                                            @*<td class="text-center"><b>Description</b></td>*@
                                                            <td class="" style="width:26%;padding:1% !important;"><p style="margin-bottom:3px !important;">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</p><p class="text-center" style="font-size:1.3em;"><b>Description</b></p></td>
                                                            <td class="" style="width:26%;padding:1% !important;"><p style="margin-bottom:3px !important;"><b id="POACare1" style="color:red;">@POAC1</b><b id="POAFinance1" style="float:right;color:red;">@POAF1</b></p><p class="text-center" style="font-size:1.3em;"><b>1st Contact</b></p></td>
                                                            <td class="" style="width:26%;padding:1% !important;"><p style="margin-bottom:3px !important;"><b id="POACare2" style="color:red;">@POAC2</b><b id="POAFinance2" style="float:right;color:red;">@POAF2</b></p><p class="text-center" style="font-size:1.3em;"><b>2nd Contact</b></p></td>
                                                            <td class="" style="width:26%;padding:1% !important;"><p style="margin-bottom:3px !important;"><b id="POACare3" style="color:red;">@POAC3</b><b id="POAFinance3" style="float:right;color:red;">@POAF3</b></p><p class="text-center" style="font-size:1.3em;"><b>3rd Contact</b></p></td>
                                                        </tr>

                                                        <tr>
                                                            <td>Contact</td>
                                                            <td>@Model.Contract1</td>
                                                            <td>@Model.Contract2</td>
                                                            <td>@Model.Contract3</td>
                                                        </tr>
                                                        <tr>
                                                            <td>Address</td>
                                                            <td>@Model.Address1</td>
                                                            <td>@Model.Address2</td>
                                                            <td>@Model.Address3</td>
                                                        </tr>
                                                        <tr>
                                                            <td>Relationship</td>
                                                            <td>@Model.Relationship1</td>
                                                            <td>@Model.Relationship2</td>
                                                            <td>@Model.Relationship3</td>
                                                        </tr>
                                                        <tr>
                                                            <td>Primary Contact </td>
                                                            <td>@Model.First_phone1</td>
                                                            <td>@Model.First_phone2</td>
                                                            <td>@Model.First_phone3</td>
                                                        </tr>
                                                        <tr>
                                                            <td>E-Mail</td>
                                                            <td>@Model.Email1</td>
                                                            <td>@Model.Email2</td>
                                                            <td>@Model.Email3</td>
                                                        </tr>
                                                        <tr style="display:none;">
                                                            <td>Secondary Contact </td>
                                                            <td>@Model.Second_phone1</td>
                                                            <td>@Model.Second_phone2</td>
                                                            <td>@Model.Second_phone3</td>
                                                        </tr>
                                                        <tr style="display:none;">
                                                            <td>Tertiary Contact </td>
                                                            <td>@Model.Third_phone1</td>
                                                            <td>@Model.Third_phone2</td>
                                                            <td>@Model.Third_phone3</td>
                                                        </tr>

                                                    </tbody>
                                                </table>
                                                <br><br>
                                                <table class="table table-bordered">
                                                    <thead class="bg-primary">
                                                        <tr>
                                                            <th class="text-white text-center" colspan="3">Medical Contact Details</th>
                                                        </tr>
                                                    </thead>
                                                    <tbody>
                                                        <tr>
                                                            <td>Physician: @Model.Physician</td>
                                                            <td>Physician Phone: @Model.PhysicianPhone</td>
                                                            <td>Fax #: @Model.PhysicianFaxNo</td>
                                                        </tr>
                                                        <tr>
