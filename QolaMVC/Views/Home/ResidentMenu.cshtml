<style>

    .menu-box:hover .menu {
        transform: scale(1.05);
        transition: all .5s;
    }

    .pointer {
        cursor: pointer;
    }

    .nav-link.active {
        background-color: #015093 !important;
    }

    #options_in_profile button {
        border-radius: 0.3vw;
    }

    #options_in_profile a {
        border-radius: 0.3vw;
    }


    #pills-care .ptitle {
        font-family: Times,serif;
        font-size: 15px;
        color: rgb(0,0,0);
        font-weight: bold;
        font-style: normal;
        text-decoration: none;
    }
    #pills-care p {
        margin-bottom:5px !important;
    }
    #pills-care .ptitle_grey {
        font-family: Times,serif;
        font-size: 15px;
        color: rgb(0,0,0);
        font-weight: bold;
        font-style: normal;
        text-decoration: none;
        background-color: darkgrey;
        border-bottom: 2px solid black;
    }
    #pills-care .ptitle_nocolor_noborderB {
        font-family: Times,serif;
        font-size: 15px;
        color: rgb(0,0,0);
        font-weight: bold;
        font-style: normal;
        text-decoration: none;
    }
    .borderB {
        border-bottom: 2px solid black;
    }

    #pills-care .pcontent {
        font-family: Times,serif;
        font-size: 15px;
        color: rgb(0,0,0);
        font-weight: normal;
        font-style: normal;
        text-decoration: none;
    }

