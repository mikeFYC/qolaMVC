                                            <p><span class="ptitle">Tel #: </span><span class="pcontent">@Model.PhysicianPhone</span></p>
                                            <p><span class="ptitle">Fax #: </span><span class="pcontent">@Model.PhysicianFaxNo</span></p>
                                        </div>
                                        <div class="text-center" style="width:100%;">
                                            <p class="ptitle_grey">Pharmacy</p>
                                        </div>
                                        <div class="">
                                            <p><span class="ptitle">Name: </span><span class="pcontent">@Model.PharmacyName</span></p>
                                            <p><span class="ptitle">Tel #: </span><span class="pcontent">@Model.PharmacePhoneNo</span></p>
                                            <p><span class="ptitle">Fax #: </span><span class="pcontent">@Model.PharmaceFaxNumber</span></p>
                                        </div>
                                    </div>
                                    <div style="margin-left:0.5%;width:31.5%;display:inline-block;border:1px solid black;height:1100px;vertical-align:top;position:absolute;">
                                        <div class="text-center" style="width:100%;">
                                            <p class="ptitle_grey">Emergency Contacts</p>
                                        </div>
                                        <div class="borderB">
                                            <p><span class="ptitle">#1 Name: </span><span class="pcontent">@Model.Contract1</span></p>
                                            <p><span class="ptitle">Relationship: </span><span class="pcontent">@Model.Relationship1</span></p>
                                            @{
                                                if (Model.POACareStatus == 'Y')
                                                {
                                                    <p><span class="ptitle">POA Care: </span>&#9745;<span class="pcontent">Yes</span>&#9744;<span class="pcontent">No</span></p>
                                                }
                                                else
                                                {
                                                    <p><span class="ptitle">POA Care: </span>&#9744;<span class="pcontent">Yes</span>&#9745;<span class="pcontent">No</span></p>
                                                }

                                                if (Model.POAFinanceStatus == 'Y')
                                                {
                                                    <p><span class="ptitle">POA Finance: </span>&#9745;<span class="pcontent">Yes</span>&#9744;<span class="pcontent">No</span></p>
                                                }
                                                else
                                                {
                                                    <p><span class="ptitle">POA Finance: </span>&#9744;<span class="pcontent">Yes</span>&#9745;<span class="pcontent">No</span></p>
                                                }
                                            }
                                        </div>
                                        <div class="text-center" style="width:100%;">
                                            <p class="ptitle_grey">Telephone Number</p>
                                        </div>
                                        <div class="borderB">
                                            <p><span class="ptitle">1°#: </span><span class="pcontent">@(Model.First_phone1)(@(Model.First_phone_type1_text))</span></p>
                                            <p><span class="ptitle">2°#: </span><span class="pcontent">@(Model.Second_phone1)(@(Model.Second_phone_type1_text))</span></p>
                                            <p><span class="ptitle">3°#: </span><span class="pcontent">@(Model.Third_phone1)(@(Model.Third_phone_type1_text))</span></p>
                                        </div>
                                        <div class="text-center" style="width:100%;">
                                            <p class="ptitle_grey">Address</p>

                                        </div>
                                        <div class="borderB" style="height:40px;">
                                            <p><span class="pcontent">@Model.Address1</span></p>
                                        </div>
                                        <div class="borderB">
                                            <p><span class="ptitle">E-Mail: </span><span class="pcontent">@Model.Email1</span></p>
                                        </div>

