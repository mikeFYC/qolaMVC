                                        <div class="text-center" style="width:100%;">
                                            <p class="ptitle_grey">Telephone Number</p>
                                        </div>
                                        <div class="borderB">
                                            <p><span class="ptitle">1°#: </span><span class="pcontent">@(Model.First_phone3)(@(Model.First_phone_type3_text))</span></p>
                                            <p><span class="ptitle">2°#: </span><span class="pcontent">@(Model.Second_phone3)(@(Model.Second_phone_type3_text))</span></p>
                                            <p><span class="ptitle">3°#: </span><span class="pcontent">@(Model.Third_phone3)(@(Model.Third_phone_type3_text))</span></p>
                                        </div>
                                        <div class="text-center" style="width:100%;">
                                            <p class="ptitle_grey">Address</p>
                                        </div>
                                        <div class="borderB" style="height:40px;">
                                            <p><span class="pcontent">@Model.Address3</span></p>
                                        </div>
                                        <div class="">
                                            <p><span class="ptitle">E-Mail: </span><span class="pcontent">@Model.Email3</span></p>
                                        </div>

                                    </div>
                                    <div style="margin-left:32.5%;width:31.5%;display:inline-block;border:1px solid black;height:1100px;vertical-align:top;position:absolute;">
                                        <div class="text-center" style="width:100%;">
                                            <p class="ptitle_grey" style="margin-bottom:0px !important;">Level Of Care</p>
                                        </div>
                                        <div class="borderB" style="height:120px;">
                                            @{
                                                if (ViewBag.careplan.LevelOfCare_Full != "" && ViewBag.careplan.LevelOfCare_Full != null)
                                                {
                                                    <div class="borderB" style="width:100%;">
                                                        <div class="text-center" style="width:10%;display:inline-block;">&#10004;</div>
                                                        <div style="display:inline-block;border-left:1px solid black;padding-left:5px;">
                                                            <p style="margin-top:3px;"><span class="pcontent">@ViewBag.careplan.LevelOfCare_Full</span></p>
