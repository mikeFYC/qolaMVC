                                        <div style="width:99%;">
                                            <div style="width:50%;display:inline-block;"><p class="title2">QOLA - @Model.Home.Name</p></div>
                                            <div style="width:20%;display:inline-block;"></div>
                                            <div class="" style="float:right;display:inline-block;"><p class="title2">@Model.SuiteNo @Model.FirstName  @Model.LastName</p></div>
                                        </div>
