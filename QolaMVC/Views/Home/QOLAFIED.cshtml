                                        </div>
                                        <div class="text-center" style="width:100%;">
                                            <p class="ptitle_grey">Allergies</p>
                                        </div>
                                        <div class="borderB" style="height:140px;">
                                            <textarea class="pcontent" style="width:98% !important;overflow:hidden;height:auto;border:none;">@Model.Alergies</textarea>
                                        </div>
