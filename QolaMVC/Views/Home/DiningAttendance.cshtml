    function getdata() {
        var mealselect = document.getElementById("select_meal_index").className;
        var dateselect = document.getElementById("lblCalendarMsg").innerHTML;
        $.get("/Home/Getting_Dining", { whichmeal: mealselect, datesel: dateselect }, function (data) {
            var a = String(data).split(";");
            for (var c = 0; c < a.length; c++) {
                var cplus = c + 1;
                for (var b = 0; b < a[c].split(",").length; b++) {
                    if (a[c].split(",")[b] != "") {
                        var d = $("input[name=" + a[c].split(",")[b] + "][value=option" +cplus+"]");
                        d[0].checked = true;
                        var e = $("input[name=\"" + a[c].split(",")[b] + "+\"][value=option" + cplus + "]");
                        e[0].checked = true;
                    }
                }
            }
        });
        $.get("/Home/View_List", { whichmeal: mealselect, datesel: dateselect }, function (data) {
            var a = String(data).split(";");
            var view = $("#show_list");
            for (var c = 0; c < a.length; c++) {
                var cplus = c + 1;
                for (var b = 0; b < a[c].split(",").length; b++) {
                    if (a[c].split(",")[b] != "") {
                        if (c == 0)
                            view.append("<tr><td>" + a[c].split(",")[b] + "<\/td><td>Taken<\/td><td><div class=\"form-check form-check-inline\"><i class=\"fa fa-file-text-o\"><\/i><\/div><\/td><\/tr>");
                        if (c == 1)
                            view.append("<tr><td>" + a[c].split(",")[b] + "<\/td><td>Refused<\/td><td><div class=\"form-check form-check-inline\"><i class=\"fa fa-file-text-o\"><\/i><\/div><\/td><\/tr>");
                        if (c == 2)
                            view.append("<tr><td>" + a[c].split(",")[b] + "<\/td><td>Hospital<\/td><td><div class=\"form-check form-check-inline\"><i class=\"fa fa-file-text-o\"><\/i><\/div><\/td><\/tr>");
                        if (c == 3)
                            view.append("<tr><td>" + a[c].split(",")[b] + "<\/td><td>Waiver<\/td><td><div class=\"form-check form-check-inline\"><i class=\"fa fa-file-text-o\"><\/i><\/div><\/td><\/tr>");
                        if (c == 4)
                            view.append("<tr><td>" + a[c].split(",")[b] + "<\/td><td>Away<\/td><td><div class=\"form-check form-check-inline\"><i class=\"fa fa-file-text-o\"><\/i><\/div><\/td><\/tr>");
                        if (c == 5)
                            view.append("<tr><td>" + a[c].split(",")[b] + "<\/td><td>Tray Complimentary<\/td><td><div class=\"form-check form-check-inline\"><i class=\"fa fa-file-text-o\"><\/i><\/div><\/td><\/tr>");

                    }
                }
            }
        });
    }

    function previousday() {
        var monthNames = [
            "January", "February", "March",
            "April", "May", "June", "July",
            "August", "September", "October",
            "November", "December"
        ];
        var a = document.getElementById("lblCalendarMsg").innerHTML;
        var b = new Date(a);
        b.setDate(b.getDate() - 1);
        document.getElementById("lblCalendarMsg").innerHTML = monthNames[b.getMonth()] + " " + b.getDate() + ", " + b.getFullYear();

        var dateselect = document.getElementById("lblCalendarMsg").innerHTML;
        if (window.location.href.toString().includes("datesel")) {
            var indexequal = window.location.href.toString().indexOf("=");
            var newstring = window.location.href.toString().substring(0, indexequal+1);
            window.location.href = newstring + dateselect;
        }
        else
            window.location.href = window.location.href + "?datesel=" + dateselect;

    }

    function nextday() {
        var monthNames = [
            "January", "February", "March",
            "April", "May", "June", "July",
            "August", "September", "October",
            "November", "December"
        ];
        var a = document.getElementById("lblCalendarMsg").innerHTML;
        var b = new Date(a);
        b.setDate(b.getDate() + 1);
        document.getElementById("lblCalendarMsg").innerHTML = monthNames[b.getMonth()] + " " + b.getDate() + ", " + b.getFullYear();

        var dateselect = document.getElementById("lblCalendarMsg").innerHTML;
        if (window.location.href.toString().includes("datesel")) {
            var indexequal = window.location.href.toString().indexOf("=");
            var newstring = window.location.href.toString().substring(0, indexequal + 1);
            window.location.href = newstring + dateselect;
        }
        else
            window.location.href = window.location.href + "?datesel=" + dateselect;
    }


