    function list_savebutton()
    {

        var savestring = "";
        var mealselect = document.getElementById("select_meal_index").className;
        var dateselect = document.getElementById("lblCalendarMsg").innerHTML;

        var a = $("div[id=pills-profile1] tr td div input");
        for (var c = 0; c < a.length; c++) {
            if (a[c].checked == true) {
                savestring = savestring + a[c].name + "," + a[c].value + ",";
            }
        }
        $.post("/Home/saveButton_Dining", { arr: savestring, whichmeal: mealselect, datesel: dateselect}, function (data) {
            if (data == 1)
                alert("Saved Successfully");
            else
                alert("Saved failed");
        });
    }
    function floor_savebutton() {
        var savestring = "";
        var mealselect = document.getElementById("select_meal_index").className;
        var dateselect = document.getElementById("lblCalendarMsg").innerHTML;

        var a = $("div[id=pills-contact1] tr td div input");
        for (var c = 0; c < a.length; c++) {
            if (a[c].checked == true) {
                var g = String(a[c].name);
                var nameg = g.substring(0, g.length - 1);
                savestring = savestring + nameg + "," + a[c].value + ",";
                

            }
