<!-- BEGIN SIDEBAR -->
<!-- BEGIN SIDEBPANEL-->
<style>
    .mike_ul
    { 
        list-style-type: none;
        margin: 0; 
        padding: 0; 
        overflow: hidden; 
        background-color: #333; 

    }

    .mike_li {
        float: left;
    }

    .mike_li a {
        display: block;
        color: white;
        text-align: center;
        padding: 14px 16px;
        text-decoration: none;
    }

        .mike_li a:hover {
            background-color: #111;
        }
</style>

<nav>
    <!-- BEGIN SIDEBAR MENU TOP TRAY CONTENT-->
    <div class="sidebar-overlay-slide from-top" id="appMenu">

    </div>
    <!-- END SIDEBAR MENU TOP TRAY CONTENT-->
    <!-- BEGIN SIDEBAR MENU HEADER-->
    <div>
        <img src="/Content/assets/img/logo_white.png" alt="logo" class="brand" data-src="/Content/assets/img/logo_white.png" data-src-retina="/Content/assets/img/logo_white_2x.png" width="78" height="22">
        <div class="sidebar-header-controls">
            <button type="button" class="btn btn-xs sidebar-slide-toggle btn-link m-l-20" data-pages-toggle="#appMenu">
                <i class="fa fa-angle-down fs-16"></i>
            </button>
            <button type="button" class="btn btn-link hidden-sm-down" data-toggle-pin="sidebar">
                <i class="fa fs-12"></i>
            </button>
        </div>
    </div>
    <!-- END SIDEBAR MENU HEADER-->
    <!-- START SIDEBAR MENU -->
    <div>
        <!-- BEGIN SIDEBAR MENU ITEMS-->
        <ul class="mike_ul">
            <li class="mike_li">
                <div style="display:inline-block;">
                    <a href="/Home/">
                        <span class="title">Homes</span>
                    </a>
                </div>
                <div style="display:inline-block;">
                    <span class="icon-thumbnail " style="top:1vw;"><i class="pg-home"></i></span>
                </div>
            </li>
            @{
                if (ViewBag.Home != null)
                {
            <li class="mike_li">
                <div style="display:inline-block;">
                    <a href="/Home/Menu/?p_HomeId=@ViewBag.Home.Id" class="detailed">
                        <span class="title">Dashboard</span>
                    </a>
                </div>
                <div style="display:inline-block;">
                    <span class="icon-thumbnail " style="top:1vw;"><i class="fa fa-dashboard"></i></span>
                </div>
            </li>
                }
            }

            @{
                if (ViewBag.Resident != null)
                {
                    <li class="mike_li">
                        <div style="display:inline-block;">
                            <a href="/Home/ResidentMenu/?p_ResidentId=@ViewBag.Resident.ID" class="detailed">
                                <span class="title">Profile</span>
                            </a>
                        </div>
                        <div style="display:inline-block;">
                            <span class="icon-thumbnail " style="top:1vw;"><i class="fa fa-user"></i></span>
                        </div>
                    </li>
                }
            }

            @*<li class="">
                    <a href="#">
                        <span class="title">Todo List</span>
                    </a>
                    <span class="icon-thumbnail "><i class="pg-social"></i></span>
                </li>*@
                    <li class="mike_li">
                        <div style="display:inline-block;">
                            <a href="javascript:;">
                                <span class="title">Masters</span>
                                <span class=" arrow"></span>
                            </a>
                        </div>
                        <div style="display:inline-block;">
                            <span class="icon-thumbnail" style="top:1vw;"><i class="pg-grid"></i></span>
                        </div>
                            <ul class="mike_ul" style="display:none;">
                                <li class="mike_li">
                                    <a href="~/Master/Users">Users</a>
                                    <span class="icon-thumbnail"></span>
                                </li>
                                <li class="mike_li">
                                    <a href="~/Allergy/List">Allergy</a>
                                    <span class="icon-thumbnail">sp</span>
                                </li>
                                <li class="mike_li">
                                    <a href="~/Master/ActivityCategory">Activity Category</a>
                                    <span class="icon-thumbnail">sp</span>
                                </li>
                                <li class="mike_li">
                                    <a href="~/Master/Activity">Activity</a>
                                    <span class="icon-thumbnail">sp</span>
                                </li>
                                <li class="mike_li">
                                    <a href="~/DineTime/List">Dine Time</a>
                                    <span class="icon-thumbnail">sp</span>
                                </li>
                                <li class="mike_li">
                                    <a href="~/SpecialDiet/List">Special Diet</a>
                                    <span class="icon-thumbnail">sp</span>
                                </li>
                                <li class="mike_li">
                                    <a href="~/Suite/List">Suite</a>
                                    <span class="icon-thumbnail">sp</span>
                                </li>
                                <li class="mike_li">
                                    <a href="~/Venue/List">Venue</a>
                                    <span class="icon-thumbnail">sp</span>
                                </li>
                            </ul>
                    </li>
        </ul>
        <div class="clearfix"></div>
    </div>
    <!-- END SIDEBAR MENU -->
</nav>
<!-- END SIDEBAR -->
<!-- END SIDEBAR -->