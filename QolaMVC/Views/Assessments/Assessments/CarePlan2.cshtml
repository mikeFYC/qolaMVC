@model QolaMVC.Models.PlanOfCareModel

@RenderPage("~/Views/Shared/_Header.cshtml")
@RenderPage("~/Views/Shared/_SideBar.cshtml")
<style>
    .form-tabs .nav-link {
        font-size: 1rem;
    }

    .modal-xlg {
        width: 900px !important;
        max-width: 1000px !important;
    }
</style>
<!-- START PAGE-CONTAINER -->
<div class="page-container">

    @RenderPage("~/Views/Shared/_NavBar.cshtml")

    <!-- START PAGE CONTENT WRAPPER -->
    <div class="page-content-wrapper">
        <!-- START PAGE CONTENT -->
        <div class="content">
            <!-- START JUMBOTRON -->
            <div class="jumbotron w-100" style="position: fixed; z-index: 400">
                <div class="container-fluid container-fixed-lg">
                    <!-- <div class="inner"> -->
                    <div class="row align-items-center">
                        <div class="col-md-8">
                            <h3>Resident Care Assessment</h3>
                        </div>
                    </div>
                    <!-- </div> -->
                </div>
            </div>
            <!-- END JUMBOTRON -->
            <div style="height: 60px;"></div>
            <!-- START CONTAINER FLUID -->
            <div class="container-fluid container-fixed-lg">
                <!-- BEGIN PlACE PAGE CONTENT HERE -->

                <div class="card">
                    <div class="card-block">
                        <form action="/Assessments/CarePlan" method="post">
                            @Html.AntiForgeryToken()
                            <div class="row">
                                <div class="col-md-12 text-center">
                                    <h4>Assessed</h4>
                                </div>
                                <div class="col-md-4">
                                    <div class="form-group">
                                        <label for="staticEmail" class="col-form-label">Assessed</label>
                                        <select name="Assessed" class="form-control">
                                            <option>Select</option>
                                            @{
                                                if (Model.Assessed != null && Model.Assessed != string.Empty)
                                                {
                                                    <option value=@Model.Assessed>@Model.Assessed</option>
                                                }
                                            }
                                            <option value="Pre-Admin">Pre-Admin</option>
                                            <option value="5 Month Assessment">5 Month Assessment</option>
                                            <option value="Update">Update</option>
                                            <option value="Admission">Admission</option>
                                        </select>
                                    </div>
                                </div>
                            </div>
                            <br><br>
                            <div class="row">
                                <div class="col-md-12 text-center">
                                    <h4>Level Of Care</h4>
                                </div>
                                <div class="col-md-4">
                                    <div class="form-group">
                                        <label for="staticEmail" class="col-form-label">Level Of Care</label>
                                        <select name="LevelOfCare" class="form-control">
                                            @{
                                                if (Model.LevelOfCare != null && Model.LevelOfCare != string.Empty)
                                                {
                                                    <option value=@Model.LevelOfCare>@Model.LevelOfCare</option>
                                                }
                                            }
                                            <option>Select</option>
                                            <option value="Independent">Independent</option>
                                            <option value="Partial Assist">Partial Assist</option>
                                            <option value="Dependent">Dependent</option>
                                            <option value="Palliative">Palliative</option>
                                        </select>
                                    </div>
                                </div>
                            </div>
                            <br><br>
                            <div class="row">
                                <div class="col-md-12 text-center">
                                    <h4>Vital Signs</h4>
                                </div>
                                <div class="col-md-3">

                                    <div class="row">
                                        <div class="col-md-12">
                                            <h6>BP</h6>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="form-group form-group-default">
                                                <label>Systolic</label>
                                                @Html.TextBoxFor(m => m.VitalSigns.BPSystolic, new { @class = "form-control" })
                                            </div>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="form-group form-group-default">
                                                <label>Diastolic</label>
                                                @Html.TextBoxFor(m => m.VitalSigns.BPDiastolic, new { @class = "form-control" })
                                            </div>
                                        </div>
                                        <div class="col-md-12">
                                            <div class="form-group form-group-default">
                                                <label>Date Completed</label>
                                                @Html.TextBoxFor(m => m.VitalSigns.BPDateCompleted, new { @class = "form-control" })

                                            </div>
                                        </div>
                                    </div>

                                </div>
                                <div class="col-md-3">

                                    <div class="row">
                                        <div class="col-md-12">
                                            <h6>Temperature</h6>
                                            <div class="form-group form-group-default">
                                                <label>Temperature (Celsius)</label>
                                                @Html.TextBoxFor(m => m.VitalSigns.Temperature, new { @class = "form-control" })
                                            </div>
                                        </div>
                                        <div class="col-md-12">
                                            <div class="form-group form-group-default">
                                                <label>Date Completed</label>
                                                @Html.TextBoxFor(m => m.VitalSigns.TempDateCompleted, new { @class = "form-control" })
                                            </div>
                                        </div>
                                    </div>

                                </div>
                                <div class="col-md-3">

                                    <div class="row">
                                        <div class="col-md-12">
                                            <h6>Weight</h6>

                                            <div class="form-group form-group-default">
                                                <label>Weight (lbs)</label>
                                                @Html.TextBoxFor(m => m.VitalSigns.Weight, new { @class = "form-control" })
                                            </div>
                                        </div>
                                        <div class="col-md-12">
                                            <div class="form-group form-group-default">
                                                <label>Date Completed</label>
                                                @Html.TextBoxFor(m => m.VitalSigns.WeightDateCompleted, new { @class = "form-control" })
                                            </div>
                                        </div>
                                    </div>

                                </div>
                                <div class="col-md-3">
                                    <div class="row">
                                        <div class="col-md-12">
                                            <h6>Height</h6>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="form-group form-group-default">
                                                <label>Centimeters</label>
                                                @Html.TextBoxFor(m => m.VitalSigns.HeightFeet, new { @class = "form-control" })
                                            </div>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="form-group form-group-default">
                                                <label>Inches</label>
                                                @Html.TextBoxFor(m => m.VitalSigns.HeightInches, new { @class = "form-control" })
                                            </div>
                                        </div>
                                        <div class="col-md-12">
                                            <div class="form-group form-group-default">
                                                <label>Date Completed</label>
                                                @Html.TextBoxFor(m => m.VitalSigns.HeightDateCompleted, new { @class = "form-control" })
                                            </div>
                                        </div>
                                    </div>

                                </div>
                            </div>
                            <br><br>
                            <div class="row">
                                <div class="col-md-3">
                                    <div class="form-group form-group-default">
                                        <label>Pulse</label>
                                        @Html.TextBoxFor(m => m.VitalSigns.Pulse, new { @class = "form-control" })
                                    </div>
                                    <div class="form-group form-group-default">
                                        <label>Date Completed</label>
                                        @Html.TextBoxFor(m => m.VitalSigns.PulseDateCompleted, new { @class = "form-control" })
                                    </div>
                                    <div class="form-check margin">
                                        @Html.RadioButtonFor(m => m.VitalSigns.PulseRegular, "Regular", new { @class = "form-check-input" })
                                        <label class="form-check-label checkspace" for="">
                                            Regular
                                        </label>
                                    </div>
                                    <div class="form-check margin">
                                        @Html.RadioButtonFor(m => m.VitalSigns.PulseRegular, "Irregular", new { @class = "form-check-input" })
                                        <label class="form-check-label checkspace" for="">
                                            Irregular
                                        </label>
                                    </div>
                                </div>
                                <div class="col-md-3">
                                    <div class="form-group form-group-default">
                                        <label>O2-Oxygen</label>
                                        <select name="Oxygen_02" class="form-control">
                                            @{
                                                if (Model.SpecialNeeds.Oxygen != null && Model.SpecialNeeds.Oxygen != string.Empty)
                                                {
                                                    <option value=@Model.SpecialNeeds.Oxygen>@Model.SpecialNeeds.Oxygen</option>
                                                }
                                            }
                                            <option value="1">Independent</option>
                                            <option value="2">Partial Assist</option>
                                            <option value="3">Dependent</option>
                                            <option value="4">Palliative</option>
                                            }

                                            }
                                        </select>
                                    </div>
                                    <div class="form-group form-group-default">
                                        <label>Rate (L/min)</label>
                                        @Html.TextBoxFor(m => m.SpecialNeeds.OxygenRate, new { @class = "form-control" })
                                    </div>
                                </div>
                            </div>
                            <br><br>
