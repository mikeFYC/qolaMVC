                                    <b>Location:</b>
                                </div>
                                @{
                                    for(int i=0; i<Model.PersonalHygiene.PreferredDaysCollection.Count; i++)
                                    {
                                        <div class="form-check margin">
                                            @Html.CheckBoxFor(m => m.PersonalHygiene.PreferredDaysCollection[i].IsSelected, new { @class = "form-check-input" })
                                            <label class="label" for="">@Model.PersonalHygiene.PreferredDaysCollection[i].Name</label>
                                            @Html.HiddenFor(m => m.PersonalHygiene.PreferredDaysCollection[i].Name, new { @class = "label", value = Model.PersonalHygiene.PreferredDaysCollection[i].Name })
                                            @Html.HiddenFor(m => m.PersonalHygiene.PreferredDaysCollection[i].Notes, new { @class = "label", value = Model.PersonalHygiene.PreferredDaysCollection[i].Notes })
