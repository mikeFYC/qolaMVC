                                        <select name="PersonalHygiene.BathingPreferredType" class="form-control">
                                            @{
                                                if (Model.PersonalHygiene != null && Model.PersonalHygiene.BathingPreferredType != null && Model.PersonalHygiene.BathingPreferredType != string.Empty)
                                                {
                                                    <option selected="selected" value="@Model.PersonalHygiene.BathingPreferredType">@Model.PersonalHygiene.BathingPreferredType</option>
                                                }
                                            }
                                            <option value="0">Select</option>
                                            <option value="12PB">Bath</option>
