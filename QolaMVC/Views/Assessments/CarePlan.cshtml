                                                        <option selected="selected" value="@Model.PersonalHygiene.AMPreferredType">@Model.PersonalHygiene.AMPreferredType</option>
                                                    }
                                                }
                                                <option value="0">Select</option>
                                                <option value="87APB">Bath</option>
