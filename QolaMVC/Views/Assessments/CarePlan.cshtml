@model QolaMVC.Models.PlanOfCareModel
@RenderPage("~/Views/Shared/_Header.cshtml")
@RenderPage("~/Views/Shared/_SideBar.cshtml")

<style>

    .form-tabs .nav-link {
        font-size: 1rem;
    }

    .modal-xlg {
        width: 900px !important;
        max-width: 1000px !important;
    }
    .required_sign {
        color: red;
    }

    .label {
        padding-left: 20px;
        margin-bottom: 0;
        font-weight: normal;
        cursor: pointer;
        background-color:white;
    }
    #RCA_firstrow label{
        font-size:1.1vw;
    }
    .InfectiousDiseases80{
        width:80%;
    }
    .InfectiousDiseases2vh {
        margin-top:2vh;
    }
    input:disabled {
        background-color: lightgray !important;
    }
    select:disabled {
        background-color: lightgray !important;
    }
</style>


<!-- START PAGE-CONTAINER -->
<div class="page-container">

    @RenderPage("~/Views/Shared/_NavBar.cshtml")

    <!-- START PAGE CONTENT WRAPPER -->
    <div class="page-content-wrapper">
        <!-- START PAGE CONTENT -->
        <div class="content">
            <form action="~/Assessments/CarePlan" method="Post">
                 @Html.AntiForgeryToken()
                <!-- START JUMBOTRON -->
                <div class="jumbotron w-100" style="position: fixed; z-index: 400">
                    <div class="container-fluid container-fixed-lg">
                        <!-- <div class="inner"> -->
                        <div class="row align-items-center">
                            <div style="width:74%;display:inline-block;">
                                <h3>Resident Care Assessment</h3>
                            </div>
                            <div style="width:25%;display:inline-block;">
                                <a href="~/Assessments/" class="btn btn-primary"><i class="fa fa-backward"></i> Back</a>
                                <button type="button" class="btn btn-danger" id="RCA_PRINT" onclick="RC_PRINT()"><i class="fa fa-file-pdf-o"></i> Print</button>
                            </div>
                        </div>
                        <!-- </div> -->
                    </div>
                </div>
                <!-- END JUMBOTRON -->

                <div style="height: 60px;"></div>

                <div class="container">
                    <div class="col-lg-12">
                        
                    </div>
                </div>

                <input type="hidden" value="RC_Assessment" name="" class="filename">
                <!-- START CONTAINER FLUID -->
                <div class="container-fluid container-fixed-lg" id="fromHTMLtestdiv">
                    <!-- BEGIN PlACE PAGE CONTENT HERE -->
                    <div class="card">
                        <div class="card-block">
                            <div class="row" id="RCA_firstrow">
                                <div class="col-2">
                                    <label for="" class="col-form-label"><b>Resident: </b></label>
                                    <label>@ViewBag.Resident.FirstName @ViewBag.Resident.LastName </label>
                                </div>
                                <div class="col-2">
                                    <label for="" class="col-form-label"><b>Suite No: </b></label>
                                    <label>@ViewBag.Resident.SuiteNo</label>
                                </div>
                                <div class="">
                                    <label for="" class="col-form-label"><b>Assessment History:</b></label>
                                    
                                </div>
                                <div class="col-2">
                                    <select class="form-control col-12" name="AssessmentDate" id="selectdate" onchange="formsubmit(this)">

                                        @if (ViewBag.AssessmentDates != null && ViewBag.AssessmentDates[0].ToShortDateString() != "1/1/0001")
                                        {
                                            for (var ind = 0; ind < ((List<DateTime>)ViewBag.AssessmentDates).Count(); ind++)
                                            {
                                                <option value=@ind name=@(ViewBag.ID_list[ind])>@(((List<DateTime>)ViewBag.AssessmentDates)[ind].ToString("yyyy-MM-dd"))</option>
                                            }
                                        }
                                        else
                                        {
                                            <option value="0"></option>}

                                    </select>
                                    <input type="hidden" id="ID_ID" name="Id" value="" />
                                </div>
                                <div class="col-2">
                                    
                                </div>
                            </div>
                            <br>
                            <div class="row">
                                <div class="col-md-12 bg text-center">
                                    <h4>Assessed</h4>
                                </div>
                                <div class="col-md-4">
                                    <div class="form-group">
                                        <label for="" class="col-form-label">Assessed<span class="required_sign">&nbsp;*</span></label>
                                        <select id="Assessed_Alert" name="Assessed" class="form-control">
                                            @{
                                                if (Model.Assessed != null && Model.Assessed != string.Empty)
                                                {
                                                    <option selected="selected" value=@Model.Assessed>@Model.Assessed_Full</option>
                                                }
                                            }
                                            <option value="">Select</option>
                                            <option value="7PA">Pre-Admin</option>
                                            <option value="6AN">5 Month Assessment</option>
                                            <option value="2AU">Update</option>
                                            <option value="8AA">Admission</option>
                                        </select>
                                    </div>
                                </div>
                            </div>
                            <br><br>
                        </div>
                    </div>
                    <div class="card">
                        <div class="card-block">
                            <div class="row">
                                <div class="col-md-12 bg text-center">
                                    <h4>Level Of Care</h4>
                                </div>
