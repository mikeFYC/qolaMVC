@model QolaMVC.Models.PlanOfCareModel
@RenderPage("~/Views/Shared/_Header.cshtml")
@RenderPage("~/Views/Shared/_SideBar.cshtml")
<style>

    .form-tabs .nav-link {
        font-size: 1rem;
    }

    .modal-xlg {
        width: 900px !important;
        max-width: 1000px !important;
    }

    .label {
        padding-left: 20px;
        margin-bottom: 0;
        font-weight: normal;
        cursor: pointer;
    }

</style>
<!-- START PAGE-CONTAINER -->
<div class="page-container">

    @RenderPage("~/Views/Shared/_NavBar.cshtml")

    <!-- START PAGE CONTENT WRAPPER -->
    <div class="page-content-wrapper">
        <!-- START PAGE CONTENT -->
        <div class="content">
            <form action="~/Assessments/CarePlan" method="Post">
                 @Html.AntiForgeryToken()
                <!-- START JUMBOTRON -->
                <div class="jumbotron w-100" style="position: fixed; z-index: 400">
                    <div class="container-fluid container-fixed-lg">
                        <!-- <div class="inner"> -->
                        <div class="row align-items-center">
                            <div class="col-md-8">
                                <h3>Resident Care Assessment</h3>
                            </div>
                        </div>
                        <!-- </div> -->
                    </div>
                </div>
                <!-- END JUMBOTRON -->

                <div style="height: 60px;"></div>

                <div class="container">
                    <div class="col-lg-12">
                        <button class="btn btn-default print">GENERATE PDF</button>
                    </div>
                </div>

                <input type="hidden" value="familyconferencereport" name="" class="filename">
                <!-- START CONTAINER FLUID -->
                <div class="container-fluid container-fixed-lg" id="fromHTMLtestdiv">
                    <!-- BEGIN PlACE PAGE CONTENT HERE -->

                    <div class="card">
                        <div class="card-block">
                            <div class="row">
                                <div class="col-md-12 bg text-center">
                                    <h4>Assessed</h4>
                                </div>
                                <div class="col-md-4">
                                    <div class="form-group">
                                        <label for="staticEmail" class="col-form-label">Assessed</label>
                                        <select name="Assessed" class="form-control">
                                            @{
                                                if (Model.Assessed != null && Model.Assessed != string.Empty)
                                                {
                                                    <option selected="selected" value=@Model.Assessed>@Model.Assessed</option>
                                                }
                                            }
                                            <option value="0">Select</option>
                                            <option value="7PA">Pre-Admin</option>
                                            <option value="6AN">5 Month Assessment</option>
                                            <option value="2AU">Update</option>
                                            <option value="8AA">Admission</option>
                                        </select>
                                    </div>
                                </div>
                            </div>
                            <br><br>
                        </div>
                    </div>

                    <div class="card">
                        <div class="card-block">
                            <div class="row">
                                <div class="col-md-12 bg text-center">
                                    <h4>Level Of Care</h4>
                                </div>
                                <div class="col-md-4">
                                    <div class="form-group">
                                        <label for="staticEmail" class="col-form-label">Level Of Care</label>
                                        <select name="LevelOfCare" class="form-control">
                                            @{
                                                if (Model.LevelOfCare != null && Model.LevelOfCare != string.Empty)
                                                {
                                                    <option selected="selected" value=@Model.LevelOfCare>@Model.LevelOfCare</option>
                                                }
                                            }
                                            <option value="0">Select</option>
                                            <option value="3LI">Independent</option>
                                            <option value="2LP">Partial Assist</option>
                                            <option value="4LD">Dependent</option>
                                            <option value="5LPT">Palliative</option>
                                        </select>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="card">
                        <div class="card-block">
                            <br><br>
                            <div class="row">
                                <div class="col-md-12 bg text-center">
                                    <h4>Vital Signs</h4>
                                </div>
                                <div class="col-md-3">
                                    <br>
                                    <div class="row">
                                        <div class="col-md-12">
                                            <h6>BP</h6>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="form-group form-group-default">
                                                <label>Systolic</label>
                                                @Html.TextBoxFor(m => m.VitalSigns.BPSystolic, new { @class = "form-control" })
                                            </div>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="form-group form-group-default">
                                                <label>Diastolic</label>
                                                @Html.TextBoxFor(m => m.VitalSigns.BPDiastolic, new { @class = "form-control" })
                                            </div>
                                        </div>
                                        <div class="col-md-12">
                                            <div class="form-group form-group-default">
                                                <label>Date Completed</label>
                                                @Html.TextBoxFor(m => m.VitalSigns.BPDateCompleted, new { @class = "form-control" })
                                            </div>
                                        </div>
                                    </div>

                                </div>
                                <div class="col-md-3">
                                    <br>
                                    <div class="row">
                                        <div class="col-md-12">
                                            <div class="form-group form-group-default">
                                                <label>Temperature (Celsius)</label>
                                                @Html.TextBoxFor(m => m.VitalSigns.Temperature, new { @class = "form-control" })
                                            </div>
                                        </div>
                                        <div class="col-md-12">
                                            <div class="form-group form-group-default">
                                                <label>Date Completed</label>
                                                @Html.TextBoxFor(m => m.VitalSigns.TempDateCompleted, new { @class = "form-control" })
                                            </div>
                                        </div>
                                    </div>

                                </div>
                                <div class="col-md-3">
                                    <br>
                                    <div class="row">
                                        <div class="col-md-12">
                                            <div class="form-group form-group-default">
                                                <label>Weight (lbs)</label>
                                                @Html.TextBoxFor(m => m.VitalSigns.Weight, new { @class = "form-control" })
                                            </div>
                                        </div>
                                        <div class="col-md-12">
                                            <div class="form-group form-group-default">
                                                <label>Date Completed</label>
                                                @Html.TextBoxFor(m => m.VitalSigns.WeightDateCompleted, new { @class = "form-control" })
                                            </div>
                                        </div>
                                    </div>

                                </div>
                                <div class="col-md-3">
                                    <br>
                                    <div class="row">
                                        <div class="col-md-12">
                                            <h6>Height</h6>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="form-group form-group-default">
                                                <label>Centimeters</label>
                                                @Html.TextBoxFor(m => m.VitalSigns.HeightFeet, new { @class = "form-control" })
                                            </div>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="form-group form-group-default">
                                                <label>Inches</label>
                                                @Html.TextBoxFor(m => m.VitalSigns.HeightInches, new { @class = "form-control" })
                                            </div>
                                        </div>
                                        <div class="col-md-12">
                                            <div class="form-group form-group-default">
                                                <label>Date Completed</label>
                                                @Html.TextBoxFor(m => m.VitalSigns.HeightDateCompleted, new { @class = "form-control" })
                                            </div>
                                        </div>
                                    </div>

                                </div>
                            </div>
                            <br><br>
                            <div class="row">
                                <div class="col-md-3">
                                    <div class="form-group form-group-default">
                                        <label>Pulse</label>
                                        @Html.TextBoxFor(m => m.VitalSigns.Pulse, new { @class = "form-control" })
                                    </div>
                                    <div class="form-group form-group-default">
                                        <label>Date Completed</label>
                                        @Html.TextBoxFor(m => m.VitalSigns.PulseDateCompleted, new { @class = "form-control" })
                                    </div>
                                    <div class="form-check margin">
                                        @Html.RadioButtonFor(m => m.VitalSigns.PulseRegular, "Regular", new { @class = "form-check-input" })
                                        <label class="label" for="">Regular</label>
                                    </div>
                                    <div class="form-check margin">
                                        @Html.RadioButtonFor(m => m.VitalSigns.PulseRegular, "Irregular", new { @class = "form-check-input" })
                                        <label class="label" for="">Irregular</label>
                                    </div>
                                </div>
                                <div class="col-md-3">
                                    <div class="form-group form-group-default">
                                        <label>O2-Oxygen</label>
                                        <select name="SpecialNeeds.Oxygen" class="form-control">
                                            @{
                                                if (Model.SpecialNeeds != null && Model.SpecialNeeds.Oxygen != null && Model.SpecialNeeds.Oxygen != string.Empty)
                                                {
                                                    <option selected="selected" value=@Model.SpecialNeeds.Oxygen>@Model.SpecialNeeds.Oxygen</option>
                                                }
                                            }
                                            <option>Independent</option>
                                            <option>Partial Assist</option>
                                            <option>Dependent</option>
                                            <option>Palliative</option>
                                        </select>
                                    </div>
                                    <div class="form-group form-group-default">
                                        <label>Rate (L/min)</label>
                                        @Html.TextBoxFor(m => m.SpecialNeeds.OxygenRate, new { @class = "form-control" })
                                    </div>

                                </div>
                            </div>
                        </div>
                    </div>


                    <br><br>
                    <div class="card">
                        <div class="card-block">
                            <div class="row">
                                <div class="col-md-12 bg text-center">
                                    <h4>Personal Hygiene</h4>
                                </div>
                            </div>
