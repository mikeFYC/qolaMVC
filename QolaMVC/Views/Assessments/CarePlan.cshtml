                                        <div class="col-md-12">
                                            <h6>BP</h6>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="form-group form-group-default">
                                                <label>Systolic</label>
                                                @Html.TextBoxFor(m => m.VitalSigns.BPSystolic, new { @class = "form-control" })
                                            </div>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="form-group form-group-default">
                                                <label>Diastolic</label>
                                                @Html.TextBoxFor(m => m.VitalSigns.BPDiastolic, new { @class = "form-control" })
                                            </div>
                                        </div>
                                        <div class="col-md-12">
                                            <div class="form-group form-group-default">
                                                <label>Date Completed</label>
                                                @Html.TextBoxFor(m => m.VitalSigns.BPDateCompleted, new { @class = "form-control" })
                                            </div>
                                        </div>
                                    </div>

                                </div>
                                <div class="col-md-3">
                                    <br>
                                    <div class="row">
                                        <div class="col-md-12">
                                            <div class="form-group form-group-default">
                                                <label>Temperature (Celsius)</label>
                                                @Html.TextBoxFor(m => m.VitalSigns.Temperature, new { @class = "form-control" })
                                            </div>
                                        </div>
                                        <div class="col-md-12">
                                            <div class="form-group form-group-default">
                                                <label>Date Completed</label>
                                                @Html.TextBoxFor(m => m.VitalSigns.TempDateCompleted, new { @class = "form-control" })
                                            </div>
                                        </div>
                                    </div>

                                </div>
                                <div class="col-md-3">
