                                                <option value="89APS">Sponge</option>
                                            </select>
                                        </div>
                                    </div>
                                </div>

                            </div>
                            <div class="row">

                                <div class="col-md-2">
                                    <div class="form-group row">
                                        <label for="staticEmail" class="col-md-12 col-form-label">PM Care</label>
                                        <div class="col-md-12">
                                            <select name="PersonalHygiene.PMCare" class="form-control">

                                                @{
                                                    if (Model.PersonalHygiene != null && Model.PersonalHygiene.PMCare != null && Model.PersonalHygiene.PMCare != string.Empty)
                                                    {
                                                        <option selected="selected" value="@Model.PersonalHygiene.PMCare" >@Model.PersonalHygiene.PMCare</option>
                                                    }
                                                }
                                                <option value="0">Select</option>
                                                <option value="8PI">Independent</option>
                                                <option value="9PS">Supervised</option>
                                                <option value="10PA">Assisted</option>
                                                <option value="11PD">Dependent</option>
                                                <option value="72PS4">SL4</option>
                                                <option value="73PSD4">SL4D</option>
                                                <option value="74PP">Private (Supportive)</option>
                                            </select>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-2">
                                    <div class="form-group row">
                                        <label for="staticEmail" class="col-md-12 col-form-label">Assisted by</label>
                                        <div class="col-md-12">
                                            <select name="PersonalHygiene.PMAssistedBy" class="form-control">
                                                @{
                                                    if (Model.PersonalHygiene != null && Model.PersonalHygiene.PMAssistedBy != null && Model.PersonalHygiene.PMAssistedBy != string.Empty)
                                                    {
                                                        <option selected="selected" value="@Model.PersonalHygiene.PMAssistedBy">@Model.PersonalHygiene.PMAssistedBy</option>
                                                    }
                                                }
                                                <option value="0">Select</option>
                                                <option value="28PH">Home</option>
                                                <option value="29PA">Agency</option>
                                            </select>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-2">
                                    <div class="form-group row">
                                        <label for="staticEmail" class="col-md-12 col-form-label">Agency name</label>
                                        <div class="col-md-12">
                                            @Html.TextBoxFor(m => m.PersonalHygiene.PMAgencyName, new { @class = "form-control" })
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-2">
                                    <div class="form-group row">
