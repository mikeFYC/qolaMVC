                                            @Html.TextAreaFor(m => m.Medications, new { @class = "form-control textarea", rows = "5" })
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <br><br>
                            <div class="row">
                                <div class="col-md-6">
