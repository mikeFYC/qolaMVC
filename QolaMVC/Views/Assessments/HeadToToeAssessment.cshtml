                                            @Html.TextBoxFor(m => m.ReturnedFromHospital, new { @class = "form-control" })
                                        </div>
                                    </div>
                                    <div class="form-group row">
                                        <label for="" class="col-md-3 col-form-label">Diagnosis from hospital</label>
