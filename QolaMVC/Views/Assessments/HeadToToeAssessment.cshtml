@model QolaMVC.Models.AdmissionHeadToToeModel

@RenderPage("~/Views/Shared/_Header.cshtml")
@RenderPage("~/Views/Shared/_SideBar.cshtml")

<style>

    .form-tabs .nav-link {
        font-size: 1rem;
    }

    .modal-xlg {
        width: 900px !important;
        max-width: 1000px !important;
    }
</style>
<!-- START PAGE-CONTAINER -->
<div class="page-container">

    @RenderPage("~/Views/Shared/_NavBar.cshtml")
    <!-- START PAGE CONTENT WRAPPER -->
    <div class="page-content-wrapper">
        <!-- START PAGE CONTENT -->
        <div class="content">
            <!-- START JUMBOTRON -->
            <div class="jumbotron w-100" style="position: fixed; z-index: 400">
                <div class="container-fluid container-fixed-lg">
                    <!-- <div class="inner"> -->
                    <div class="row align-items-center">
                        <div class="col-md-9">
                            <h3>Admission Head to Toe Assessment</h3>
                        </div>
                        <div class="col-md-2">
                            <a href="~/Assessments/" class="btn btn-primary"><i class="fa fa-backward"></i> Back</a>
                            <button class="btn btn-primary print" id="HTT_PRINT"><i class="fa fa-file-pdf-o"></i> Print</button>
                        </div>
                    </div>
                    <!-- </div> -->
                </div>
            </div>
            <!-- END JUMBOTRON -->
            <div style="height: 60px;"></div>
            <!-- START CONTAINER FLUID -->
            <div class="container-fluid container-fixed-lg" id="fromHTMLtestdiv">
                <input type="hidden" value="AHTT_Assessment" name="" class="filename">
                <!-- BEGIN PlACE PAGE CONTENT HERE -->
                <div class="card">
                    <div class="card-block">
                        <div class="row" id="HTT_firstrow">
                            <div class="col-2">
                                Assessment History:
                            </div>
                            <div class="col-2">
                                <select class="form-control col-12" name="AssessmentDate" id="selectdate" onchange="formsubmit(this)">

                                    @if (ViewBag.AssessmentDates != null && ViewBag.AssessmentDates[0].ToShortDateString() != "1/1/0001")
                                    {
                                        for (var ind = 0; ind < ((List<DateTime>)ViewBag.AssessmentDates).Count(); ind++)
                                        {
                                            <option value=@ind>@(((List<DateTime>)ViewBag.AssessmentDates)[ind].ToString("yyyy-MM-dd"))</option>
                                        }
                                    }
                                    else
                                    {
                                        <option value="0"></option>}

                                </select>
                            </div>
                            <div class="col-2">
                                <button class="btn btn-danger" type="button" onclick="location.href='@Url.Action("Add_HeadToToeAssessment_mike", "Assessments")'">NEW</button>
                            </div>
                        </div>
                        <hr>
                        <br>
                        <form action="~/Assessments/AddHeadToToeAssessment" method="post">
                            <div class="row">
                                <div class="col-md-8">
                                    <div class="form-group">
                                        <div class="row">
                                            <label class="col-md-5 col-form-label">Admission Status</label>
                                            <div class="col-md-7 pads">
                                                <div class="form-check form-check-inline movesp">
                                                    @Html.RadioButtonFor(m => m.AdmissionStatus, "New Admission", new { @class = "form-check-input" })
                                                    <label class="form-check-label" for="inlineRadio1">New Admission</label>
                                                </div>
                                                <div class="form-check form-check-inline">
                                                    @Html.RadioButtonFor(m => m.AdmissionStatus, "Re-Admission", new { @class = "form-check-input" })
                                                    <label class="form-check-label" for="inlineRadio2">Re-Admission</label>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-12">
                                    <h5><u>HISTORY OF RE-ADMISSION ONLY</u></h5>
                                </div>
                                <div class="col-md-12">
                                    <div class="form-group row">
                                        <label for="" class="col-md-3 col-form-label">Returned from (hospital)</label>
